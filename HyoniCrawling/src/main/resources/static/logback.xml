<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appender name="consoleAppender" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
           	<Pattern>[%-5level][%thread][%d{HH:mm:ss.SSS}] %logger{5} - %msg%n</Pattern>
		</encoder>
	</appender>
	
	<root level="INFO">
		<appender-ref ref="consoleAppender"/>
	</root>
</configuration>